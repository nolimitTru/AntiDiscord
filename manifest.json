{
    "manifest_version": 2,
    "name": "AntiDiscord",
    "description": "A Extension to stop you from opening discord when busy!",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "*://*.discord.com/*"
            ],
            "js": [
                "main.js"
            ]
        }
    ]
}